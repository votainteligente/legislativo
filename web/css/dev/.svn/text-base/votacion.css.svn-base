/********* VERTICAL TAB *********/
.vtab {
    margin: auto;
    width: 350px;
	float: left;
    height: 100%;
}
.vtab > ul > li {
    width: 110px;
    height: 40px;
    background-color: #ffffff;
    list-style-type: none;
    display: block;
    text-align: center;
    margin: auto;
    padding-bottom: 10px;
    border: 1px solid #fff;
    position: relative;
    border-right: none;
    opacity: .3;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=30)";
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=30);
}
.vtab > ul > li.votos_si {
    background: url('../images/votacion/1si_on.png') no-repeat center center;
}
.vtab > ul > li.votos_no {
    background: url('../images/votacion/2no_on.png') no-repeat center center;
}
.vtab > ul > li.votos_aus {
    background: url('../images/votacion/3aus_on.png') no-repeat center center;
}
.vtab > ul > li.votos_pareo {
    background: url('../images/votacion/4pareo_on.png') no-repeat center center;
}
.vtab > ul > li.votos_abs {
    background: url('../images/votacion/5abs_on.png') no-repeat center center;
}
.vtab > ul > li.selected {
    opacity: 1;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=100);
    position: relative;
}
.vtab > ul {
    float: left;
    width: 110px;
    text-align: left;
    display: block;
    margin: auto 0;
    padding: 0;
    position: relative;
    top: 30px;
}

.vtab > div {
    margin-left: 83px;
    border: 2px solid #f2f2f2;
    min-height: 300px;
	padding-top: 5px;
    position:relative;
    z-index: 9;
	-moz-border-radius-bottomleft: 5px;
	-moz-border-radius-topleft: 5px;
	width: 260px;
}
.vtab  div  h4 {
    color: #898989;
    font-size:9pt;
    border-bottom: 2px dashed #f2f2f2;
	font-weight:bold;
}


.category .feed
{
  margin: 4px 15px;
  float: right;
  line-height: 25px;
  background-position: left center;
}

.category .feed a
{
  color: #fff;
  text-decoration: none;
  text-transform: uppercase;
}
.proyectos
{
 width: 760px;
 margin: 0 auto;
}

.proyectos tr.even
{
  background-color: #f3fafc;
}

.proyectos tr.odd
{
}

.proyectos tr:hover
{
  background-color: #fff;
}

.proyectos td
{
  padding: 12px 15px;
  border-bottom: 1px dotted #999;
}

.proyectos td.id,
.proyectos td.nroboletin,
.proyectos td.titulo,
.proyectos td.fechaingreso,
.proyectos td.iniciativa,
.proyectos td.tipo,
.proyectos td.camaraorigen,
.proyectos td.urgencia,
.proyectos td.ley,
.proyectos td.decreto,
.proyectos td.materia
{
  border-right: 1px dotted #999;
}

.proyectos .nroboletin
{
  color: #524F46;
  font-weight: bold;
  width: 30%;
}

.more_proyectos
{
  text-align: right;
  text-transform: uppercase;
  font-size: 85%;
  margin: 5px 15px 15px;
  clear: both;
}

.more_proyectos a
{
  color: #176f94;
  text-decoration: none;
  font-weight: bold;
  font-size: 130%;
}

.more_proyectos a:hover
{
  color: #176f94;
  text-decoration: underline;
}

.pagination_desc
{
  margin-left: 15px;
  margin-top: 10px;
  font-style: italic;
  color: #828f95;
}

.pagination_desc strong
{
  color: #176f94;
}

.pagination
{
  margin-top: 10px;
  float: right;
  margin-right: 10px;
  color: #176f94;
  font-size: 110%;
  font-weight: bold;
}

.pagination a
{
  margin: 0 1px;
  text-decoration: none;
  color: #828f95;
  font-size: 75%;
  vertical-align: 1px;
  font-weight: normal;
}

.pagination a:hover
{
  text-decoration: underline;
}

.pagination img
{
  vertical-align: -2px;
}
